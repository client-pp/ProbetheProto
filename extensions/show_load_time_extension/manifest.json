{
  "name": "Show Load Time Extension",
  "description": "This is an extension that shows loading time",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Show Load Time Extension"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["content.js"]
    }
  ]
}
